<%= form_for @user, url: users_path, html: { multipart: true } do |f| %>
	<%= f.label :first_name %><%= f.text_field :first_name %><br>
	<%= f.label :last_name %> <%= f.text_field :last_name %><br>
  <%= f.file_field :avatar %>
	<%= image_tag @user.avatar.url %>
	<br>
	<%= f.submit 'Submit' %>
  
<% end %>